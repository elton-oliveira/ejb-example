<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="3.1" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
	<display-name>EJB-Example</display-name>

	<enterprise-beans>
		<session>
			<ejb-name>ShoppingCartBean</ejb-name>
			<!-- Add an entry in the JNDI ENC from ShoppingCartBean-->
			<env-entry>
				<env-entry-name>finishShoppingMessage</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>Order finalized!</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
</ejb-jar>